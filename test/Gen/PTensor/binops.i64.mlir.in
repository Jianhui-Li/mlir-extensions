// NUMPLACEHOLDERS 2 NUMVARIANTS 12
// PLACEHOLDER OPID 0 2 4 6 8 18 19 20 21 23 24 25
// PLACEHOLDER RES 16 1 15 14  4 13  3  1 39 2197 10 4
// op names: add bitwise_and bitwise_or bitwise_xor floordivide maximum minimum modulo mult power subtract div
// RUN: %python_executable %imex_runner -i %s -f %p/ptensor.pp -e main -entry-point-result=void --shared-libs=%mlir_c_runner_utils --shared-libs=%mlir_runner_utils | FileCheck %s

func.func private @printMemrefI64(%ptr : tensor<*xi64>)

func.func @main() {
    %i5 = arith.constant 5 : index
    %c2 = arith.constant 3 : i64
    %c13 = arith.constant 13 : i64

    %0 = ptensor.create %i5 value %c13 {dtype = 2 : i8} : (index, i64) -> !ptensor.ptensor<?xi64>
    %1 = ptensor.create %i5 value %c2 {dtype = 2 : i8} : (index, i64) -> !ptensor.ptensor<?xi64>

    call @test_op(%0, %1) : (!ptensor.ptensor<?xi64>, !ptensor.ptensor<?xi64>) -> ()

    return
}

func.func @test_op(%a : !ptensor.ptensor<?xi64>, %b : !ptensor.ptensor<?xi64>) {
    %0 ="ptensor.ewbin"(%a, %b) {op = @OPID@ : i32} : (!ptensor.ptensor<?xi64>, !ptensor.ptensor<?xi64>) -> !ptensor.ptensor<?xi64>
    %1 = builtin.unrealized_conversion_cast %0 : !ptensor.ptensor<?xi64> to tensor<?xi64>
    %2 = tensor.cast %1 : tensor<?xi64> to tensor<*xi64>
    call @printMemrefI64(%2) : (tensor<*xi64>) -> ()
    // CHECK: Unranked Memref base@ = {{(0x)?[-9a-f]*}}
    // CHECK-SAME: rank = 1 offset = 0 sizes = [5] strides = [1] data =
    // CHECK-NEXT: [@RES@,  @RES@,  @RES@,  @RES@,  @RES@]

    return
}
